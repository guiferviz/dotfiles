# Git aliases.
alias gs="git status"
alias gpp='git pull origin "$(git symbolic-ref --short HEAD)"'
alias gp='git push origin "$(git symbolic-ref --short HEAD)"'
alias gcod="git checkout develop"
alias gcom="git checkout master"
alias pr="poetry run"
alias prv="poetry run nvim"
alias prn="poetry run nvim"
alias prp="poetry run python"
alias prpt="poetry run pytest"

# Dotfiles.
# Refer to https://github.com/guiferviz/dotfiles/tree/main to learn how to use
# this alias.
alias dotfiles='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'
